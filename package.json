{
  "name": "co-compose",
  "version": "5.1.3",
  "description": "Adonis and koa style middleware layer with ability to run parallel middleware",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "node japaFile.js",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "prepublishOnly": "npm run build",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "commit": "git-cz",
    "lint": "eslint . --ext=.ts",
    "release": "np",
    "version": "npm run build"
  },
  "keywords": [
    "adonisjs",
    "koa",
    "middleware",
    "co",
    "co-middleware"
  ],
  "author": "amanvirk,adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^2.2.4",
    "@types/node": "^13.7.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-adonis": "^1.0.6",
    "husky": "^4.2.1",
    "istanbul": "^0.4.5",
    "japa": "^3.0.1",
    "japa-cli": "^1.0.1",
    "mrm": "^2.0.4",
    "np": "^5.2.1",
    "pkg-ok": "^2.3.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinss/co-compose.git"
  },
  "bugs": {
    "url": "https://github.com/poppinss/co-compose/issues"
  },
  "homepage": "https://github.com/poppinss/co-compose#readme",
  "np": {
    "contents": ".",
    "anyBranch": false
  }
}
